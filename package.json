{
  "name": "sketch-git-pro",
  "version": "0.1.4",
  "engines": {
    "sketch": ">=3.0"
  },
  "resources": [
    "Resources/branches.js"
  ],
  "skpm": {
    "name": "sketch-git-pro",
    "manifest": "src/manifest.json",
    "main": "sketch-git-pro.sketchplugin",
    "assets": [
      "assets/**/*",
      "src/i18n/*"
