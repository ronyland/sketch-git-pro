    "sketch-module-fs": "^0.1.2",
    "sketch-module-google-analytics": "^0.1.3",
    "sketch-module-update": "^0.1.2",
    "sketch-module-user-preferences": "^1.0.1",
    "sketch-module-web-view": "^0.1.4"
  },
  "author": "xuming <coolchyni@gmail.com>"
